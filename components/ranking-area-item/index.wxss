/* components/ranking-area-item/index.wxss */
.container{
  height: 200rpx;
  display: flex;
  margin: 20rpx 0;
  background-color: rgb(238, 238, 238);
  border-radius: 3%;
  align-items: center;
  overflow: hidden;
}
.container .left-item{
  padding-left: 40rpx;
  flex: 1;
}
.left-item .title{
  font-size: 17px;
  font-weight: 500;
  padding: 6rpx;
}
.song-item{
  display: flex;
}
.song-item{
   font-size: 13px;
   padding: 6rpx;
   color: #333;
}
.container .image{
   width: 200rpx;
   height: 100%;
}
.container image{
  width: 100%;
  height: 100%;
}

.singer-name{
  color: rgba(155, 150, 150, 0.959);
}

